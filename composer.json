{
  "name": "remont",
  "description": "",
  "version": "1.0",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "artifact",
      "url": "composer-artifacts/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "johnpbloch/wordpress": "4.9.6",
    "composer/installers": "~1.0",
    "wp-cli/wp-cli" : "1.5.1",
    "psy/psysh" : "~0.6",
    "imagine/imagine": "~0.5.0",
    "advanced-custom-fields-pro": "5.5.3",
    "wp-all-import-pro": "4.5",
    "wp-all-export-pro": "1.4.7",
    "wpai-acf-add-on": "3.1.5",
    "wpai-user-add-on": "1.0.9",
    "wp-media/wp-rocket": "2.11.4",
    "polylang-pro": "2.2.8",
    "comment-rating-field-pro-plugin": "3.4.9",
    "wpackagist-plugin/contact-form-7": "5.0.2",
    "wpackagist-plugin/wordpress-seo": "7.6.1",
    "wpackagist-plugin/regenerate-thumbnails": "3.0.2",
    "wpackagist-plugin/yoast-seo-settings-xml-csv-import": "1.1.7",
    "wpackagist-plugin/query-monitor": "3.0.1",
    "wpackagist-plugin/advanced-cron-manager": "2.3.2",
    "wpackagist-plugin/amazon-s3-and-cloudfront": "1.4",
    "wpackagist-plugin/wp-hide-dashboard": "2.2.1",
    "wpackagist-plugin/wp-mail-bank": "3.0.63",
    "wpackagist-plugin/memcached-redux": "0.1.6",
    "wpackagist-plugin/social-sharing-icons": "2.4",
    "wpackagist-plugin/wordpress-special-characters-in-usernames": "1.2",
    "wpackagist-plugin/imagify": "1.8.0.1"
  },
  "extra": {
    "installer-paths": {
      "public/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "public/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": {
      "johnpbloch/wordpress-core": "public/wp"
    }
  },
  "scripts" : {
    "post-install-cmd" : [
      "rm -f cli/wp",
      "ln -s ../vendor/wp-cli/wp-cli/bin/wp cli/wp",
      "rm -f public/wp-content/object-cache.php",
      "ln -s ./plugins/memcached-redux/object-cache.php public/wp-content/object-cache.php"
    ]
  }
}